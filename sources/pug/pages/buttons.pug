extends  ../_skeletonblock variables	- pageName = 'Buttons'//- Pageblock page	include ../sections/_header	+breadcrumbs-header({		text: "Choose Your Style"	})	='\n'		// Main Button types	section.section.section-lg.bg-default.text-center		.container-fluid			h3 Main Button types			.custom-wrap-1				.group-md.group-middle.button-group					.button.button-primary default size					.button.button-sm.button-primary middle size					.button.button-xs.button-primary small size					.button.button-dark default size					.button.button-sm.button-dark middle size					.button.button-xs.button-dark small size	="\n"		// Buttons with left Icon	section.section.section-lg.bg-default.text-center		.container-fluid			h3 Buttons with left Icon			.custom-wrap-1				.group-md.group-middle.button-group					.button.button-icon.button-icon-left.button-primary						span.icon.mdi.mdi-magnify						| Left icon					.button.button-icon.button-icon-left.button-sm.button-primary						span.icon.mdi.mdi-magnify						| Left icon					.button.button-icon.button-icon-left.button-xs.button-primary						span.icon.mdi.mdi-magnify						| Left icon					.button.button-icon.button-icon-left.button-dark						span.icon.mdi.mdi-magnify						| Left icon					.button.button-icon.button-icon-left.button-sm.button-dark						span.icon.mdi.mdi-magnify						| Left icon					.button.button-icon.button-icon-left.button-xs.button-dark						span.icon.mdi.mdi-magnify						| Left icon	="\n"		// Buttons with right Icon	section.section.section-lg.bg-default.text-center		.container-fluid			h3 Buttons with right Icon			.custom-wrap-1				.group-md.group-middle.button-group					.button.button-icon.button-icon-right.button-primary						| Right icon						span.icon.mdi.mdi-arrow-right					.button.button-icon.button-icon-right.button-sm.button-primary						| Right icon						span.icon.mdi.mdi-arrow-right					.button.button-icon.button-icon-right.button-xs.button-primary						| Right icon						span.icon.mdi.mdi-arrow-right					.button.button-icon.button-icon-right.button-dark						| Right icon						span.icon.mdi.mdi-arrow-right					.button.button-icon.button-icon-right.button-sm.button-dark						| Right icon						span.icon.mdi.mdi-arrow-right					.button.button-icon.button-icon-right.button-xs.button-dark						| Right icon						span.icon.mdi.mdi-arrow-right	="\n"		// Alternative Buttons with left Icon	section.section.section-lg.bg-default.text-center		.container-fluid			h3 Buttons with shadow			.custom-wrap-1				.group-md.button-group					.button.button-primary.button-shadow default size					.button.button-sm.button-primary.button-shadow middle size					.button.button-xs.button-primary.button-shadow small size					.button.button-dark.button-shadow default size					.button.button-sm.button-dark.button-shadow middle size					.button.button-xs.button-dark.button-shadow small size	="\n"	// Page Footer	include ../sections/_footer